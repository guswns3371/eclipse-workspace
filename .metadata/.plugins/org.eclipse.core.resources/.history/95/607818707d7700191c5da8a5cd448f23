package java0516;

public class test05 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a,b,c;
		a=2;
		b=0;
		try {
			c= a/b;
			System.out.println("c="+c);
		} catch (ArithmeticException e) {
			// TODO: handle exception
			System.out.println(e.getMessage());
		}
		
		System.out.println("프로그램 실행 완료");
	}

}
